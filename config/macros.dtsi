macro_e_accent_aigu: macro_e_accent_aigu{
	compatible = "zmk,behavior-macro";
	label = "é";
	#binding-cells = <0>;
	bindings = <&macro_press   &kp LALT>
		, <&macro_tap     &kp KP_N0 &kp KP_N2 &kp KP_N3 &kp KP_N3>
		, <&macro_release &kp LALT>
		;
};
