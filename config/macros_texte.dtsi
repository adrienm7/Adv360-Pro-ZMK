// A
txt_ation: txt_ation {
	compatible = "zmk,behavior-macro";
	label = "txt_ation";
	wait-ms = <30>;
	tap-ms = <40>;
	#binding-cells = <0>;
	bindings = <&macro_tap &kp OPT7_A &kp OPT7_T &kp OPT7_I &kp OPT7_O &kp OPT7_N>;
};

